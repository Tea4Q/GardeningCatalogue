
  "version": "2.0.0",
  "tasks": [
    {
      "label": "startExpo",
      "type": "shell",
      "command": "npm",
      "args": [
        "run",
        "dev"
      ],
      "isBackground": true,
      "problemMatcher": [
        {
          "pattern": [
            {
              "regexp": ".",
              "file": 1,
              "location": 2,
              "message": 3
            }
          ],
          "background": {
            "activeOnStart": true,
            "beginsPattern": "^.*Starting Metro Bundler.*$",
            "endsPattern": "^.*Logs for your project will appear below.*$"
          }
        }
      ],
      "presentation": {
        "reveal": "always",
        "panel": "new"
      }
    }
  ]
}